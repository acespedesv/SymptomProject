<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="oval">
            <stroke android:width="4dp" android:color="#025E06"/>
            <size android:width="100dp" android:height="100dp"/>
            <gradient android:angle="90" android:startColor="@color/colorMainGreen"
                android:endColor="#025E06"/> <!--TODO: add this color to xml-->
        </shape>
    </item>

    <item>
        <shape android:shape="line">
            <stroke android:color="@color/colorMainWhite" android:width="10dp"/>
        </shape>
    </item>

</layer-list>